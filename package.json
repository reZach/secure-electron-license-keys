  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reZach/secure-electron-license-keys/issues"
  },
  "homepage": "https://github.com/reZach/secure-electron-license-keys#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-preset-minify": "^0.5.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "esm": "^3.2.25"
  }
}
